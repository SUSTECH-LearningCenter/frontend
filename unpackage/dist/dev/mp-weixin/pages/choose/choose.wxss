page {
  height: 100%;
  width: 100%;
}
.content {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content .part1 {
  background-color: #dd4200;
  width: 100%;
  height: 80%;
}
.content .part1 .title {
  color: white;
  height: 10%;
  width: 80%;
  margin-left: 10%;
  margin-right: 10%;
  margin-top: 10%;
  margin-bottom: 5%;
  font-size: 50rpx;
  text-align: center;
}
.content .part1 .square {
  color: white;
  height: 60%;
  margin-left: 10%;
  margin-right: 10%;
  margin-bottom: 5%;
  border: #ffffff solid;
  border-radius: 10%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content .part1 .square .system {
  margin-top: 20%;
  font-size: 40rpx;
  text-align: center;
}
.content .part1 .square .teacher {
  border: #ffffff solid;
  border-radius: 5%/20%;
  width: 80%;
  height: 20%;
  margin-left: 10%;
  margin-right: 10%;
  margin-top: 10%;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.content .part1 .square .student {
  border: #ffffff solid;
  border-radius: 5%/20%;
  width: 80%;
  height: 20%;
  margin-left: 10%;
  margin-right: 10%;
  margin-top: 10%;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.content .part2 {
  background-color: #ffffff;
  width: 100%;
  height: 20%;
}
.content .part2 .bottom_info {
  margin-top: 10%;
  font-size: 25rpx;
  text-align: center;
  color: #000000;
}

